    <Style x:Key="MessageBubbleStyle" TargetType="ContentControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ContentControl">
                    <Grid Margin="-8,0,0,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="8"/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Canvas VerticalAlignment="Bottom" Width="8" Height="10" Visibility="{Binding IsLast, Converter={StaticResource BooleanToVisibilityConverter}}">
                            <Path Canvas.Top="8" Width="8" Height="2" Data="F1M0,2L8,2 8,0 0,0z" Fill="{ThemeResource MessageBorderBrush}" StrokeThickness="0" />
                            <Path Width="8" Height="8" Data="F1M8,0L0,8 8,8" Fill="{ThemeResource MessageBackgroundBrush}" StrokeThickness="0" />
                        </Canvas>
                        <ContentPresenter Grid.Column="1"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="MessageBubbleOutStyle" TargetType="ContentControl">
        <Setter Property="Template">
