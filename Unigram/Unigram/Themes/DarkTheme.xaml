                        </Grid.ColumnDefinitions>
                        <ContentPresenter/>
                        <Canvas VerticalAlignment="Bottom" Width="8" Height="10" Visibility="{Binding IsLast, Converter={StaticResource BooleanToVisibilityConverter}}" Grid.Column="1">
                            <Path Canvas.Top="8" Width="8" Height="2" Data="F1M0,2L8,2 8,0 0,0z" Fill="{ThemeResource MessageBorderBrush}" StrokeThickness="0" />
                            <Path Width="8" Height="8" Data="F1M0,0L0,8 8,8z" Fill="{ThemeResource MessageBackgroundBrush}" StrokeThickness="0" />
                        </Canvas>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
	
